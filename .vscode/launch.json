{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch (GDB)",
            "preLaunchTask": "build",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/App.exe",
            "args": [],
            "MIMode": "gdb",
            "miDebuggerPath": "c:/tools/mingw64/bin/gdb.exe",
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}